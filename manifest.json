{
  "manifest_version": 2,

  "name": "PupUp",
  "description": "This extension shows cute animal pictures",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts":
  [{
    "matches": ["file:///index.html"],
    "js": ["js/jQuery-v3.1.0.js", "js/pinterestConnection.js"]
  }]
}